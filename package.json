{
    "name": "econos-dashboard",
    "version": "0.1.0",
    "private": true,
    "engines": {
        "npm": "please-use-yarn",
        "yarn": ">=1.22.18",
        "node": ">=16.0.0"
    },
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "eslint .",
        "format": "prettier --config .prettierrc 'src/**/**/*.{ts,js,tsx,jsx}' --write && git add -A .",
        "test": "jest --passWithNoTests",
        "cypress": "concurrently \"yarn dev\" \"npx cypress open\"",
        "prepare": "husky install",
        "storybook": "start-storybook -p 6006 -s ./public",
        "build-storybook": "build-storybook",
        "pre-commit": "yarn run lint && yarn run format && yarn run test",
        "chromatic": "npx chromatic --project-token=tokenID"
    },
    "dependencies": {
        "@chakra-ui/icons": "^2.0.2",
        "@chakra-ui/react": "^2.1.2",
        "@emotion/react": "^11.9.0",
        "@emotion/styled": "^11.8.1",
        "@reduxjs/toolkit": "^1.8.4",
        "@testing-library/react": "^13.3.0",
        "eslint-plugin-jest": "^26.5.3",
        "framer-motion": "^6.3.4",
        "i18next": "^21.9.2",
        "jest-environment-jsdom": "^28.1.2",
        "next": "12.1.6",
        "next-i18next": "^12.0.1",
        "next-redux-wrapper": "^7.0.5",
        "react": "18.1.0",
        "react-dom": "18.1.0",
        "react-i18next": "^11.18.6",
        "react-redux": "^8.0.2",
        "swr": "^1.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.18.2",
        "@babel/preset-env": "^7.18.2",
        "@babel/preset-typescript": "^7.17.12",
        "@chakra-ui/storybook-addon": "^4.0.9",
        "@mdx-js/react": "^2.1.1",
        "@storybook/addon-a11y": "^6.5.10",
        "@storybook/addon-actions": "^6.5.10",
        "@storybook/addon-essentials": "^6.5.10",
        "@storybook/addon-interactions": "^6.5.10",
        "@storybook/addon-links": "^6.5.10",
        "@storybook/addon-storyshots": "^6.5.10",
        "@storybook/addon-storyshots-puppeteer": "^6.5.10",
        "@storybook/addons": "^6.5.10",
        "@storybook/builder-webpack5": "^6.5.10",
        "@storybook/manager-webpack5": "^6.5.10",
        "@storybook/mdx2-csf": "^0.0.3",
        "@storybook/react": "^6.5.10",
        "@storybook/testing-library": "^0.0.13",
        "@storybook/theming": "^6.5.10",
        "@testing-library/jest-dom": "^5.16.4",
        "@types/jest": "^27.5.1",
        "@types/node": "17.0.36",
        "@types/react": "^18.0.15",
        "@types/react-dom": "18.0.5",
        "@types/react-redux": "^7.1.24",
        "@typescript-eslint/eslint-plugin": "^5.25.0",
        "@typescript-eslint/parser": "^5.25.0",
        "babel-jest": "^28.1.0",
        "babel-loader": "^8.2.5",
        "chromatic": "^6.5.6",
        "concurrently": "^7.6.0",
        "cypress": "^12.7.0",
        "eslint": "8.16.0",
        "eslint-config-next": "12.1.6",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-cypress": "^2.12.1",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "flag-icons": "^6.6.2",
        "husky": "^8.0.1",
        "jest": "^28.1.0",
        "lint-staged": "^12.4.1",
        "prettier": "^2.6.2",
        "puppeteer": "^3.3.0",
        "storybook-addon-next": "^1.6.7",
        "ts-jest": "^28.0.3",
        "typescript": "4.7.2"
    },
    "lint-staged": {
        "**/*.{js,ts,jsx,tsx}": [
            "yarn run lint",
            "yarn run format",
            "yarn run test"
        ],
        "*.js": "eslint --cache --fix",
        "*.--write": "prettier --write"
    }
}
