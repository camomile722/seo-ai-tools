{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint", "jsx-a11y"],
    "rules": {
        "no-unused-vars": "warn",
        "no-console": "warn"
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:jsx-a11y/recommended",
        "plugin:jest/recommended",
        "plugin:cypress/recommended",
        "prettier"
    ],
    "env": {
        "jest/globals": true
    }
}
